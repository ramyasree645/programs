import java.util.Scanner;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       int s=11;
       System.out.print(s*s+" ");
       for(int i=1;i<=n;i++)
       {
           s=s+4;
           System.out.print(s*s+" ");
       }
         
    }
}
