import java.util.Scanner;
public class Max
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int max=0;
        int n= sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
        a[i]=sc.nextInt();
        if (a[i]>max)
        {
        max=a[i];
        }
        }
        System.out.println(max);
    }
}
