import java.util.Scanner;
public class Factorial
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int i;
        int n=sc.nextInt();
        for(i=1;i<=n;i++)
        {
             n=n/i;
        }
        if(n==1)
         System.out.println("yes");
         else 
          System.out.println("no");
    }   
}
