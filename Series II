import java.util.Scanner;
public class Series2
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int pri=6,diff=5;
        for(int i=1;i<=n;i++)
        {
        System.out.print(pri+" ");
         pri=pri+diff;
            diff=diff+5;
        }
        
    }
}
