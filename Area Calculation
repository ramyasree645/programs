import java.util.Scanner;
public class Area
{
    public static void main(String args[])
    {
     Scanner sc=new Scanner(System.in);   
        int x=sc.nextInt();
        int y=sc.nextInt();
        int z=sc.nextInt();
        float r=sc.nextFloat();
        System.out.println(x*x);
        System.out.println(y*z);
        System.out.printf("%.2f",(3.14*r*r));
    }
}
