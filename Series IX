import java.util.*;
public class Series9
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        long n=sc.nextInt();
        long s=2;
        System.out.print(2+" ");
        for(int i=1;i<=n;i++)
        {
            s=s*s-1;

        System.out.print(s+" ");
        }
    }
}
