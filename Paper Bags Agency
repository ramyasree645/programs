import java.util.Scanner;
public class Paper
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int w=sc.nextInt();
        int x=sc.nextInt();
        int y=sc.nextInt();
        int z=(w*(x-y)-100);
        System.out.println(z);
        
        //write your code
    }
}
