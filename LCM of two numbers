import java.util.Scanner;
public class Lcm
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int s;
        s=(a>b)?a:b;
        while(true)
        {
            if(s%a==0&&s%b==0)
            {
                System.out.printf("Lcm of %d and %d is %d",a,b,s);
                break;
            }
            ++s;
        }
        //Write your code
    }
}
