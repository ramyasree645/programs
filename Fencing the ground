import java.util.Scanner;
public class Fence
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int l=sc.nextInt();
        int b=sc.nextInt();
        int a=2*(l+b);
        char m='m';
        String s="sqm";
        System.out.println("Required length is " + a + '\t' + m);
        System.out.println("Required quantity of carpet is " + (l*b) + "\t" + s);
    }
}
