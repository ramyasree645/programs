import java.util.Scanner;
public class SumDiff
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in); 
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c= a+b;
        int d=a-b;
        System.out.println(a+b);
        System.out.println(a-b);//write your code
    }
}
