import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
           float a= (x1+x2)/2;
           double b=(y1+y2)/2.0;
        System.out.println(a);
        System.out.println(b);
        //write your code
    }
}
