import java.util.Scanner;
public class Count
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int count=0;
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
            if(a[i]%2==0)
            count=count+1;
        }
        int f=n-count;
        System.out.printf("odd:%d\n",f);
        System.out.printf("even:%d\n",count);
    }
}
